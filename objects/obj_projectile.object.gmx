  case projectile_sonic:
   image_index = 2;
    break;
  case projectile_steelbreaking:
   image_index = 1;
     if tile_projectile(p) {
     switch p {
        case tile_brokenbrick:  tile_replace(x div 8,y div 8,tile_none); if team&gt;=0 audio_play_sound(snd_brick,0,0);         break;
