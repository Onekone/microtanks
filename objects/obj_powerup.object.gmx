            <kind>1</kind>
            <string>instance_destroy();
global.Score[other.team]+=500;
 p = instance_create(x+4,y+4,obj_textpopup);
 p.t = '500';
 p.timer = 45;
audio_play_sound(snd_itemuse,0,0);

switch powerup {
    case powerup_star:
        if global.PowerUp[other.team]&amp;3 &lt; 3
            global.PowerUp[other.team]++; 
        break;
    case powerup_clock: 
        /*with obj_enemytank {
            x = (x+4)&amp;$fffffffc;
            y = (y+4)&amp;$fffffffc;
        }*/
        global.TimeStop=600; break; 
    case powerup_shovel:
        global.BunkerUp = 1200;
        with obj_defensetarget {
            var _x = x div 8, _y = y div 8;
            for(i=-1;i&lt;3;i++) {
             tile_replace(_x+i,_y-1,tile_steel);
             tile_replace(_x+i,_y+2,tile_steel);
             tile_replace(_x-1,_y+i,tile_steel);
             tile_replace(_x+2,_y+i,tile_steel);
             }
        }
        break;
    case powerup_life:
        audio_stop_sound(snd_itemuse);
        audio_play_sound(snd_extralife,0,0);
        global.Lives[other.team]++; break;
    case powerup_shield:
        other.invulnerable = 600; break;
    case powerup_grenade:
        global.temp = other.team;
        with obj_enemytank {
            
            p = instance_create(x+4,y+4,obj_textpopup);
            p.t = scoreValue;
            p.c = p_white;
            p.timer = 20;
            
            p = instance_create(x+4,y+4,obj_particle)
            p.sprite_index = spr_explosionbig;
            global.Score[global.temp]+=scoreValue;
            global.RoundKills[self.type,global.temp]++;
            global.EnemiesGoal--;
            if  global.EnemiesGoal&lt;=0 {
             with obj_controller 
                round_clear_timer = 300;
             }
        }
        audio_play_sound(snd_kill,0,0);
        instance_destroy(obj_enemytank,1);
        break;
    case powerup_dualshot:
            global.PowerUp[other.team] = global.PowerUp[other.team]&amp;3 | 1&lt;&lt;2; 
        break;
    case powerup_sonic:
            global.PowerUp[other.team] = global.PowerUp[other.team]&amp;3 | 2&lt;&lt;2;         
        break;                
    case powerup_quadshot:
            global.PowerUp[other.team] = global.PowerUp[other.team]&amp;3 | 3&lt;&lt;2;         
        break;
    case powerup_powershot:
            global.PowerUp[other.team] = global.PowerUp[other.team]&amp;3 | 4&lt;&lt;2; 
        break;            
}
</string>
